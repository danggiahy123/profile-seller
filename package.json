{
  "name": "profile-seller-webadmin",
  "version": "1.0.0",
  "description": "Web admin interface for profile-seller project",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node backend/test-server.js",
    "server:dev": "nodemon backend/test-server.js",
    "start": "concurrently \"npm run build:css && node backend/test-server.js\" \"npm run dev\"",
    "build:css": "npx tailwindcss -i ./scripts/base.css -o ./scripts/style.css --config ./config/tailwind.config.js",
    "watch:css": "npx tailwindcss -i ./scripts/base.css -o ./scripts/style.css --config ./config/tailwind.config.js --watch"
  },
  "keywords": [
    "admin",
    "web",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^5.4.20"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.0",
    "nodemon": "^3.1.10"
  }
}
